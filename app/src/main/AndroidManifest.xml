<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mancillajair.app.peek">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_peek"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo_peek"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".menu.walker_message"></activity>
        <activity android:name=".menu.walker_profile" />
        <activity android:name=".menu.Messages" />
        <activity android:name=".menu.Profile" />
        <activity android:name=".MainWalker" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity android:name=".menu.Dog" />
        <activity android:name=".menu.Search" />
        <activity android:name=".clases.ItemNearWalker" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>